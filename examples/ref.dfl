mut A: [int]
mut B: int
mut C: [[int]]

chan C1: &A[1..*]
chan C2: &A[*..*]
chan C3: &{A[*..*][2], B}
chan C4: &A

proc test1() | out C1, out C2, out C3 =
    send &A[1..5] -> C1
    send &A[2..4] -> C2
    send &A[2..4][2] -> C3

proc test2() | write A[3], in C1 =
    recv r <- C1
    write 0 -> *r[2]

proc test3() | write A, write B, in C3 =
    recv r <- C3
    write 0 -> *r
    write 0 -> B

proc test4() | write A[1], in C4 =
    recv r <- C4
    write 0 -> *r[1]
